<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>connect44fx</application>
	<version>1</version>
	
	<sessions-enabled>true</sessions-enabled>
		
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	
	<static-files>
        <include path="javafx/*.*" />
        <include path="javafx/lib/*.*" />
        <include path="connect/*.htm" />
        <include path="connect4/*.*" />
        <include path="connect4/lib/*.*" />
    </static-files>
</appengine-web-app>